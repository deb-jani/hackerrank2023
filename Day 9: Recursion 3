import java.io.*;
import java.util.*;
public class solution
{
    public static int factorial(int n)
    {
        return(n>1)?n * factorial(n-1):1;
    }
    public static void main(String[] argh)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        sc.close();
        System.out.println(factorial(n));
    }
}
